{
  newtonian_vae: {
    dim_x: 2,
    transition_std: 0.25,

    // Use KL(q(x|I)‖N (0, 1))
    // paper: we added an additional regularization term to the latent space, KL(q(x|I)‖N (0, 1))
    regularization: false,
  },

  newtonian_vae_derivation: {
    dim_xhat: 32,
    dim_pxhat_middle: 32,
  },

  general: {
    // 今のとこは reacher2d.max_episode_length 同じ値にする。
    steps: 120,

    // Use NewtonianVAECellDerivation
    derivation: true,
  },

  train: {
    device: "cuda", // "cpu", "cuda"
    dtype: "float32", // "float16", "float32"

    // Use data as episode data: index from data_start to data_stop-1
    data_start: 0,
    data_stop: 300,

    batch_size: 20,
    grad_clip_norm: 1000, // Number type or null (None)  ex. 1000
    learning_rate: 1e-3,

    epochs: 500,
    save_per_epoch: 10,

    // 保存しているモデルを読み込んで学習を再開できる
    resume: false, // こいつはargparseのほうがええなぁ
  },

  path: {
    model: "saves",
    result: "results",
    data: "data",
  },
}
