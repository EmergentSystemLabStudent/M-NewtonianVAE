    batch_size: 5,
    grad_clip_norm: null,
    learning_rate: 3e-4,
    epochs: 300,
    save_per_epoch: 50,
    resume: false,
  },

  path: {
    data: "pointmass_big/data",
    model: "pointmass_big/saves",
    result: "pointmass_big/results",
  },
}
