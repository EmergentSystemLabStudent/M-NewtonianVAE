{
  // NewtonianVAE, NewtonianVAEDerivation, NewtonianVAEV2, NewtonianVAEV2Derivation
  model: "NewtonianVAEV2",

  // recommended
  NewtonianVAEV2: {
    dim_x: 2,
    transition_std: 0.001,
    encoder_dim_middle: 1024,
    encoder_std_function: "softplus",
    regularization: false,
    // fix_abc: [0, 0, 1],
    fix_abc: null,
    decoder_type: "VisualDecoder64",
  },

  NewtonianVAE: {
    dim_x: 2,
    transition_std: 0.001,
    encoder_dim_middle: 1024,
    encoder_std_function: "softplus",
    regularization: false,
    fix_abc: [0, 0, 1],
    decoder_type: "VisualDecoder64",
  },

  NewtonianVAEDerivation: {
    dim_x: 2,
    transition_std: 0.001,
    encoder_dim_middle: 1024,
    encoder_std_function: "softplus",
    regularization: false,
    fix_abc: [0, 0, 1],
    decoder_type: "VisualDecoder64",

    dim_xhat: 32,
    dim_pxhat_middle: 32,
    pxhat_std_function: "softplus",
  },

  NewtonianVAEV2Derivation: {
    dim_x: 2,
    transition_std: 0.001,
    encoder_dim_middle: 1024,
    encoder_std_function: "softplus",
    regularization: false,
    fix_abc: [0, 0, 1],
    decoder_type: "VisualDecoder64",

    dim_xhat: 32,
    dim_pxhat_middle: 32,
    pxhat_std_function: "softplus",
  },

  train: {
    data_start: 0,
    data_stop: 1000,
    batch_size: 20,
    epochs: 300,
    learning_rate: 3e-4,
    max_time_length: 100,
    device: "cuda",
    dtype: "float32",
    // kl_annealing: true,
    save_per_epoch: 20,
  },

  external: {
    data_path: "environment/reacher2d/data",
    save_path: "environment/reacher2d/saves",
  },

  eval: {
    device: "cuda",
    dtype: "float32",
    data_start: 1000,
    data_stop: 1050,
    training: false, // model.eval() == mode.train(False)
    result_path: "environment/reacher2d/results",
  },

  ControlSuiteEnvWrap: {
    env: "reacher2d-hard",
    seed: 1,
    max_episode_length: 100,
    action_repeat: 1,
    bit_depth: 8,
    // action_type: "default",
    // position_wrap: "endeffector",
  },
