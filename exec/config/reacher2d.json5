{
  model: "NewtonianVAEV2",

  NewtonianVAEV2: {
    dim_x: 2,
    transition_std: 0.001,
    encoder_dim_middle: 1024,
    encoder_std_function: "softplus",
    // regularization: true,
    fix_abc: [0, 0, 1],
  },

  train: {
    data_start: 0,
    data_stop: 1000,
    batch_size: 5,
    epochs: 300,
    learning_rate: 3e-4,
    max_time_length: 100,
    device: "cuda",
    dtype: "float32",

    // kl_annealing: true,

    save_per_epoch: 20,
  },
}
