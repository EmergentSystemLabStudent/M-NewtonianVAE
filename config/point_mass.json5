{
  model: "NewtonianVAEV2",

  NewtonianVAEV2: {
    dim_x: 2,
    regularization: false,
    camera_name: "camera0",

    velocity: {
      fix_abc: [0, 0, 1],
      // fix_abc: null,
    },

    transition: {
      std: 0.001,
      // std: null,
    },

    encoder: {
      dim_middle: 1024,
      std_function: "softplus",
      model: "VisualEncoder64",
      model_kwargs: {
        activation: "ReLU",
      },
    },

    decoder: {
      model: "SpatialBroadcastDecoder64",
    },
  },

  train: {
    data_start: 0,
    data_stop: 1000,
    batch_size: 20,
    epochs: 300,
    learning_rate: 3e-4,
    max_time_length: 100,
    device: "cuda",
    dtype: "float32",
    kl_annealing: true,
    save_per_epoch: 20,
    check_value: false,
  },

  valid: {
    data_start: 1000,
    data_stop: 1200,
    batch_size: 20,
  },

  test: {
    device: "cuda",
    dtype: "float32",
    data_start: 1200,
    data_stop: 1400,
  },

  ControlSuiteEnvWrap: {
    env: "point_mass-easy",
    seed: 1,
    max_episode_length: 100,
    action_repeat: 1,
    bit_depth: 8,
    action_type: "random_walk",
    imgsize: [64, 64],
    task_settings: {
      target_size: 0.015,
      target_position: "default",
    },
  },

  path: {
    data_dir: "environments/point_mass/data",
    saves_dir: "environments/point_mass/saves",
    results_dir: "environments/point_mass/results",
  },
}
