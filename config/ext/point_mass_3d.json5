{
  model: "MNVAE",

  MNVAE: {
    regularization: false,
    camera_names: ["top", "x side"],

    velocity: {
      dim_x: 3,
      fix_abc: [0, 0, 1],
      // fix_abc: null,
    },

    transition: {
      std: 0.001,
      // std: null,
    },

    encoder: {
      dim_x: 3,
      std_function: "softplus",
      modellist: [
        {
          model: "VisualEncoder64",
          model_kwargs: {
            dim_output: 80,
            activation: "ReLU",
          },
        },
        {
          model: "VisualEncoder64",
          model_kwargs: {
            dim_output: 80,
            activation: "ReLU",
          },
        },
      ],
    },

    decoder: {
      dim_x: 3,
      split_version: "v1",
      modellist: [
        {
          model: "SpatialBroadcastDecoder64",
          model_kwargs: {
            dim_input: 80,
          },
        },
        {
          model: "SpatialBroadcastDecoder64",
          model_kwargs: {
            dim_input: 80,
          },
        },
      ],
    },
  },

  train: {
    data_start: 0,
    data_stop: 1000,
    batch_size: 10,
    epochs: 600,
    learning_rate: 3e-4,
    max_time_length: 100,
    device: "cuda",
    dtype: "float32",
    kl_annealing: false,
    save_per_epoch: 20,
    check_value: false,
    grad_clip_norm: 1000,
  },

  valid: {
    data_start: 1000,
    data_stop: 1200,
    batch_size: 10,
  },

  test: {
    device: "cuda",
    dtype: "float32",
    data_start: 1200,
    data_stop: 1400,
  },

  ControlSuiteEnvWrap: {
    env: "point_mass_3d-easy",
    seed: 1,
    max_episode_length: 100,
    action_repeat: 1,
    bit_depth: 8,
    action_type: "random_walk",
    imgsize: [64, 64],
    task_settings: {
      target_size: 0.015,
      target_position: "default",
    },
  },

  path: {
    data_dir: "environments/point_mass_3d/data",
    saves_dir: "environments/point_mass_3d/saves",
    results_dir: "environments/point_mass_3d/results",
  },

  reconstruction: {
    episodes: 3,
    format: "mp4",
  },

  correlation: {
    episodes: 50,
    all: true,
    format: ["png"],
  },
}
