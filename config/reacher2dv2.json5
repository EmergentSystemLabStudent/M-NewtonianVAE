{
  model: "NewtonianVAEV2",

  NewtonianVAEV2: {
    regularization: false,

    velocity: {
      dim_x: 2,
      fix_abc: [0, 0, 1],
      // fix_abc: null,
      activation: "ReLU",
    },

    transition: {
      std: 0.001,
      // std: null,
    },

    encoder: {
      dim_x: 2,
      dim_middle: 1024,
      std_function: "softplus",
      model: "VisualEncoder64",
      model_kwargs: {
        activation: "ReLU",
      },
    },

    decoder: {
      dim_x: 2,
      std: 1,
      // std: null,
      model: "VisualDecoder64",
      model_kwargs: {
        dim_middle: 1024,
        activation: "ReLU",
      },
    },
  },

  train: {
    data_start: 0,
    data_stop: 1000,
    batch_size: 20,
    epochs: 300,
    learning_rate: 3e-4,
    max_time_length: 100,
    device: "cuda",
    dtype: "float32",
    // kl_annealing: true,
    save_per_epoch: 20,
    check_value: false, // if false, a little faster
  },

  eval: {
    device: "cuda",
    dtype: "float32",
    data_start: 1000,
    data_stop: 1050,
  },

  ControlSuiteEnvWrap: {
    env: "reacher2d-hard",
    seed: 1,
    max_episode_length: 100,
    action_repeat: 1,
    bit_depth: 8,
    imgsize: [64, 64], // (H, W)
    // action_type: "default",
    // position_wrap: "endeffector",
    task_settings: {
      "init_position" : "default",
      "target_position" : "default"
    }
  },

  path: {
    data_dir: "environments/reacher2d/datav2",
    saves_dir: "environments/reacher2d/savesv2",
    results_dir: "environments/reacher2d/resultsv2",
  },
}
