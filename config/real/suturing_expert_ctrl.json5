{
  model: "PControl",

  PControl: {
    N: 3,
    dim_x: 3,
    dim_pi_middle: 2,
    std: 0.1,
    each_k: false,
  },

  train: {
    path: ["../data/suturing_expert_/1/{0..99}_*.pickle"],
    batch_size: 2,
    epochs: 10000,
    learning_rate: 1e-4,
    max_time_length: 250,
    device: "cuda",
    dtype: "float32",
    save_per_epoch: 1000,
    check_value: true,
    // gradscaler_args: {},
    // use_autocast: true,
    load_all: true,
  },

  valid: {
    path: ["../data/suturing_expert_/1/{0..99}_*.pickle"],
    // batch_size: null,
  },

  test: {
    path: ["../data/suturing_expert_/1/{0..99}_*.pickle"],
    device: "cuda",
    dtype: "float32",
  },

  path: {
    saves_dir: "../save/suturing_expert_ctrl/saves",
    results_dir: "../save/suturing_expert_ctrl/results",
    hidden_conf: "config/_hidden_conf.json5",
  },

  others: {
    // unet_mask: "save/unet/unet_weight.pth",

    keypaths: [
      "action", //
      "delta",
      "position",
      ["camera", "ee1"],
      ["camera", "ee2"],
    ],

    HandyForImage: {
      size: [224, 224],
      out_range: [0, 1],
      // src_channel_order: "CHW",
      // src_reverse_color_order: true,
      out_reverse_color_order: true,
    },

    // no_correlation_each_epoch: true,
    post_valid_sample_batch_size: "all",
  },
}
