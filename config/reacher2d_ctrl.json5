{
  model: "PControl",

  PControl: {
    N: 3,
    dim_x: 2,
    dim_pi_middle: 2,
    std: 0.1,
    // std: null,
    each_k: false,
  },

  train: {
    data_start: 15,
    data_stop: 30,
    batch_size: 2,
    epochs: 10000,
    learning_rate: 1e-4,
    max_time_length: 100,
    device: "cuda",
    dtype: "float32",
    save_per_epoch: 2000,
    check_value: true,
  },

  eval: {
    device: "cuda",
    dtype: "float32",
  },

  preprocess: {
    // scale_x: [0, 4],
  },

  ControlSuiteEnvWrap: {
    env: "reacher2d-hard",
    seed: 1,
    max_episode_length: 100,
    action_repeat: 1,
    bit_depth: 8,
    imgsize: [64, 64],
    // action_type: "default",
    // position_wrap: "endeffector",

    // (Optional) null = default init position = random
    // In reacher2d:
    //   zero, limit_red_side, red, green, yellow, limit_yellow_side
    init_position: "limit_red_side",
  },

  path: {
    data_dir: "environments/reacher2d/data_operate",
    saves_dir: "environments/reacher2d/saves_ctrl",
    results_dir: "environments/reacher2d/results_ctrl",
  },
}
